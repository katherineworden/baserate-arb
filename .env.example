# Anthropic API (required for base rate agent)
ANTHROPIC_API_KEY=your_anthropic_api_key

# Kalshi API (optional - for Kalshi markets)
KALSHI_API_KEY=your_kalshi_api_key
KALSHI_PRIVATE_KEY_PATH=path/to/kalshi_private_key.pem

# Polymarket (optional - for Polymarket markets)
POLYMARKET_API_KEY=your_polymarket_api_key
POLYMARKET_API_SECRET=your_polymarket_api_secret
POLYMARKET_API_PASSPHRASE=your_polymarket_passphrase
